{
  "name": "@svebcomponents/build",
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "check": "tsc --noEmit",
    "lint": "pnpm /^lint:.*/",
    "lint:eslint": "eslint .",
    "lint:format": "prettier --check .",
    "fix": "pnpm /^fix:.*/",
    "fix:eslint": "eslint . --fix",
    "fix:format": "prettier --write ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "@svebcomponents/ssr": "^0.0.4",
    "rollup": "^4.0.0"
  },
  "devDependencies": {
    "@svebcomponents/typescript-config": "workspace:*",
    "@svebcomponents/eslint-config": "workspace:*",
    "@svebcomponents/prettier-config": "workspace:*",
    "@types/node": "catalog:",
    "rollup": "catalog:"
  },
  "dependencies": {
    "typescript": "catalog:",
    "tslib": "catalog:",
    "rollup-plugin-svelte": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/plugin-typescript": "catalog:",
    "@rollup/plugin-virtual": "catalog:",
    "@svebcomponents/ssr": "workspace:*",
    "@svebcomponents/auto-options": "workspace:*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
